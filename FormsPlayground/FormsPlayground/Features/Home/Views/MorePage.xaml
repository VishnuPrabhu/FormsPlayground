<?xml version="1.0" encoding="utf-8"?>

<pages:BasePage
    x:TypeArguments="viewModels:MoreViewModel"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:resources="clr-namespace:FormsPlayground.Resources;assembly=FormsPlayground"
    xmlns:pages="clr-namespace:FormsPlayground.Infrastructure.Pages;assembly=FormsPlayground"
    xmlns:viewModels="clr-namespace:FormsPlayground.Features.Home.ViewModels"
    x:Class="FormsPlayground.Features.Home.More.MorePage"
    Title="{x:Static resources:Strings.Menu_More}">
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{x:Static resources:Strings.Menu_More}"
                   VerticalOptions="CenterAndExpand"
                   HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
    
</pages:BasePage>